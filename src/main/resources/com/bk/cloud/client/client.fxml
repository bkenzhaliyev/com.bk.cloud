<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bk.cloud.client.ClientController">
   <children>
      <HBox>
         <children>
            <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="411.0" prefWidth="666.0">
               <HBox>
                  <VBox.margin>
                     <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                  </VBox.margin>
               </HBox>
               <HBox prefHeight="379.0" prefWidth="666.0">
                  <children>
                      <ListView fx:id="listClient" prefHeight="231.0" prefWidth="342.0">
                        <padding>
                           <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                        </padding>
                     </ListView>
                     <ListView fx:id="listServer" prefHeight="231.0" prefWidth="352.0">
                        <padding>
                           <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                        </padding>
                     </ListView>
                  </children>
               </HBox>
               <HBox prefHeight="23.0" prefWidth="666.0">
                  <children>
                     <Button fx:id="upload" mnemonicParsing="false" text="Загрузить" onAction="#upload" />
                     <Button fx:id="download" mnemonicParsing="false" text="Выгрузить" onAction="#download" />
                     <Button fx:id="delete" mnemonicParsing="false" text="Удалить" />
                     <Button fx:id="exit" mnemonicParsing="false" text="Выход" />
                  </children>
               </HBox>
            </VBox>
         </children>
      </HBox>
   </children>
</HBox>
