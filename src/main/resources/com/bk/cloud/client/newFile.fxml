<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="83.0" prefWidth="294.0"
            xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.bk.cloud.client.NewFileController">
    <children>
        <HBox layoutX="5.0" layoutY="3.0" prefHeight="33.0" prefWidth="283.0">
            <children>
                <TextField fx:id="newFileName" alignment="TOP_LEFT" prefHeight="33.0" prefWidth="283.0" />
            </children>
        </HBox>
        <HBox layoutX="5.0" layoutY="43.0" prefHeight="25.0" prefWidth="283.0" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="43.0">
            <children>
                <Button fx:id="btnNewFile" mnemonicParsing="false" onAction="#newFile" HBox.hgrow="ALWAYS" maxWidth="Infinity" text="Создать"/>
                <Button fx:id="btnNewDir" mnemonicParsing="false" onAction="#newDir" HBox.hgrow="ALWAYS" maxWidth="Infinity" text="Создать"/>
                <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#cancel" HBox.hgrow="ALWAYS" maxWidth="Infinity" text="Отменить" />
            </children>
        </HBox>

    </children>
</AnchorPane>
